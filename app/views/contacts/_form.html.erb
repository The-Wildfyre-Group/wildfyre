<%= form_for @contact do |f| %>
<%= "First Name" %>
<%= f.text_field :first_name %>
<%= "Middle Name" %>
<%= f.text_field :middle_name %>
<%= "Last Name" %>
<%= f.text_field :last_name %><br><br>
<%= "Company" %>
<%= f.text_field :company %>
<%= "Title" %>
<%= f.text_field :title %><br><br>
<%= "Personal Email" %>
<%= f.text_field :personal_email %>
<%= "Work Email" %>
<%= f.text_field :work_email %>
<%= "Cell Phone" %>
<%= f.text_field :cell_phone %>
<%= "Work Phone" %>
<%= f.text_field :work_phone %><br><br>
<%= "Website" %>
<%= f.text_field :website %>
<%= "LinkedIn" %>
<%= f.text_field :linkedin %>
<%= "Facebook" %>
<%= f.text_field :facebook %>
<%= "Twitter" %>
<%= f.text_field :twitter %><br><br>
<%= "Street Address" %>
<%= f.text_field :street_address %>
<%= "City" %>
<%= f.text_field :city %>
<%= "State" %>
<%= f.text_field :state %>
<%= "Zip Code" %>
<%= f.text_field :zip_code %>
<%= "Country" %>
<%= f.text_field :country %><br><br>
<%= "Undergraduate School" %>
<%= f.text_field :undergraduate_school %>
<%= "Undergraduate Degree" %>
<%= f.text_field :undergraduate_degree %>
<%= "Undergraduate Year" %>
<%= f.date_field :undergraduate_year %><br><br>
<%= "Graduate School" %>
<%= f.text_field :graduate_school %>
<%= "Graduate Degree" %>
<%= f.text_field :graduate_degree %>
<%= "Graduate Year" %>
<%= f.date_field :graduate_year %><br><br>
<%= "Birthday" %>
<%= f.date_field :birthday %><br><br>
<%= "Married" %>
<%= f.check_box :married %><br><br>
<%= "Kids" %>
<%= f.text_area :kids %>


<br><br><%= f.submit %>
<% end %>